<?xml version="1.0"?>
<ejb-jar version="3.0"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd">
    <enterprise-beans>
        <session>
            <ejb-name>SessionBean</ejb-name>
            <business-remote>ejb.interceptor.method.desc.ejb.workbydd.Work</business-remote>
            <ejb-class>ejb.interceptor.method.desc.ejb.workbydd.WorkBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Bean</transaction-type>
        </session>
        <message-driven>
            <ejb-name>MDBBean</ejb-name>
            <ejb-class>ejb.interceptor.method.desc.ejb.workbydd.MDBBean</ejb-class>
            <messaging-type>javax.jms.MessageListener</messaging-type>
            <transaction-type>Bean</transaction-type>
            <message-destination-type>javax.jms.Queue</message-destination-type>
        </message-driven>
        <message-driven>
            <ejb-name>MDBBeanExcluded</ejb-name>
            <ejb-class>ejb.interceptor.method.desc.ejb.workbydd.MDBBeanExcluded</ejb-class>
            <messaging-type>javax.jms.MessageListener</messaging-type>
            <transaction-type>Bean</transaction-type>
            <message-destination-type>javax.jms.Queue</message-destination-type>
        </message-driven>
    </enterprise-beans>
    <interceptors>
    	<description>JEUS Interceptor Test</description>
    	<interceptor>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.DefaultInterceptor1</interceptor-class>
    	</interceptor>
    	<interceptor>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.DefaultInterceptor2</interceptor-class>
    		<around-invoke>
    			<method-name>intercept</method-name>
    		</around-invoke>
    	</interceptor>
    	<interceptor>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.WorkBean</interceptor-class>
    		<around-invoke>
    			<method-name>beanIntercept</method-name>
    		</around-invoke>
    	</interceptor>
    	<interceptor>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.FirstInterceptor</interceptor-class>
    		<around-invoke>
    			<method-name>intercept</method-name>
    		</around-invoke>
    	</interceptor>
    	<interceptor>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.SecondInterceptor</interceptor-class>
    		<around-invoke>
    			<method-name>intercept</method-name>
    		</around-invoke>
    	</interceptor>
    	<interceptor>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.LastInterceptor</interceptor-class>
    		<around-invoke>
    			<method-name>intercept</method-name>
    		</around-invoke>
    	</interceptor>
    	<interceptor>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.TerminatorInterceptor</interceptor-class>
    		<around-invoke>
    			<method-name>intercept</method-name>
    		</around-invoke>
    	</interceptor>
    	<interceptor>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.MDBBean</interceptor-class>
    		<around-invoke>
    			<method-name>mdbInterceptor</method-name>
    		</around-invoke>
    	</interceptor>
    	<interceptor>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.MDBBeanExcluded</interceptor-class>
    		<around-invoke>
    			<method-name>mdbInterceptor</method-name>
    		</around-invoke>
    	</interceptor>

    </interceptors>
    <assembly-descriptor>
    	<interceptor-binding>
    		<ejb-name>*</ejb-name>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.DefaultInterceptor1</interceptor-class>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.DefaultInterceptor2</interceptor-class>
    	</interceptor-binding>
    	<interceptor-binding>
    		<ejb-name>SessionBean</ejb-name>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.WorkBean</interceptor-class>
    		<method>
    			<method-name>doBeanInterceptor</method-name>
    		</method>
    	</interceptor-binding>
    	<interceptor-binding>
    		<ejb-name>SessionBean</ejb-name>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.WorkBean</interceptor-class>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.FirstInterceptor</interceptor-class>
    		<method>
    			<method-name>doOneInterceptorClass</method-name>
    		</method>
    	</interceptor-binding>

    	<interceptor-binding>
    		<ejb-name>SessionBean</ejb-name>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.WorkBean</interceptor-class>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.FirstInterceptor</interceptor-class>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.SecondInterceptor</interceptor-class>
    		<method>
    			<method-name>doTwoInterceptorClass</method-name>
    		</method>
    	</interceptor-binding>
    	<interceptor-binding>
    		<ejb-name>SessionBean</ejb-name>
	    	<interceptor-order>
	    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.WorkBean</interceptor-class>
	    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.FirstInterceptor</interceptor-class>
	    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.SecondInterceptor</interceptor-class>
	    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.TerminatorInterceptor</interceptor-class>
	    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.LastInterceptor</interceptor-class>
    		</interceptor-order>
    		<method>
    			<method-name>doTerminateInterceptorClass</method-name>
    		</method>
    	</interceptor-binding>
    	<interceptor-binding>
    		<ejb-name>SessionBean</ejb-name>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.FirstInterceptor</interceptor-class>
    		<exclude-default-interceptors>true</exclude-default-interceptors>
    		<method>
    			<method-name>doExcludeDefaultInterceptorsByDD</method-name>
    		</method>
    	</interceptor-binding>
    	<interceptor-binding>
    		<ejb-name>SessionBean</ejb-name>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.FirstInterceptor</interceptor-class>
    		<method>
    			<method-name>doExcludeDefaultInterceptorsByAnno</method-name>
    		</method>
    	</interceptor-binding>
    	
		<!--  MDB 설정  -->
    	<interceptor-binding>
    		<ejb-name>MDBBean</ejb-name>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.MDBBean</interceptor-class>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.FirstInterceptor</interceptor-class>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.SecondInterceptor</interceptor-class>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.TerminatorInterceptor</interceptor-class>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.LastInterceptor</interceptor-class>
    		<method>
    			<method-name>onMessage</method-name>
    		</method>
    	</interceptor-binding>
		<interceptor-binding>
    		<ejb-name>MDBBeanExcluded</ejb-name>
    		<interceptor-class>ejb.interceptor.method.desc.ejb.workbydd.MDBBeanExcluded</interceptor-class>
    		<exclude-default-interceptors>true</exclude-default-interceptors>
    		<method>
    			<method-name>onMessage</method-name>
    		</method>
    	</interceptor-binding>
    </assembly-descriptor>
</ejb-jar>
